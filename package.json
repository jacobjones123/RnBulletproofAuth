{
  "name": "RnBulletproofAuth",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "clean:ios": "watchman watch-del-all || true && rm -rf node_modules ios/Pods ios/Podfile.lock ios/build ~/Library/Developer/Xcode/DerivedData && npm install && npx pod-install ios",
    "lint": "eslint .",
    "start": "react-native start",
    "start:clean": "npm start -- --reset-cache",
    "test": "jest",
    "detox:ios:build:debug": "npx detox build -c ios.sim.debug",
    "detox:ios:test:debug": "npx detox test -c ios.sim.debug"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native/new-app-screen": "0.80.2",
    "@react-navigation/native": "^7.1.17",
    "@react-navigation/native-stack": "^7.3.25",
    "@tanstack/react-query": "^5.85.3",
    "react": "19.1.0",
    "react-native": "0.80.2",
    "react-native-safe-area-context": "^5.6.0",
    "react-native-screens": "^4.14.1"
  },
  "devDependencies": {
    "@babel/core": "^7.28.3",
    "@babel/preset-env": "^7.25.3",
    "@babel/runtime": "^7.25.0",
    "@react-native-community/cli": "19.1.1",
    "@react-native-community/cli-platform-android": "19.1.1",
    "@react-native-community/cli-platform-ios": "19.1.1",
    "@react-native/babel-preset": "0.80.2",
    "@react-native/eslint-config": "0.80.2",
